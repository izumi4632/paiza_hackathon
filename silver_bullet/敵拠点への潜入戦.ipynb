    "def reverse(s):\n",
    "    return \"1\" if s==\"0\" else \"0\"\n",
    "def change(s, l, r):\n",
    "    return s[:l] + \"\".join(map(reverse,list(s[l:r]))) + s[r:]\n",
    "\n",
    "s=input()\n",
    "n=int(input())\n",
    "for i in range(n):\n",
    "    l,r=map(int,input().split())\n",
    "    s=change(s,l-1,r)\n",
    "print(s)"
   ]
  }
